  "devDependencies": {
    "@types/node": "^20.6.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/jest": "^29.5.5",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0"
  },
  "engines": {
    "node": ">=16.0.0"
