{
  "name": "network-cmdb-frontend-dashboard",
  "version": "1.0.0",
  "description": "Network CMDB Frontend Dashboard with MySQL integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:health": "ts-node src/utils/db-health-check.ts"
  },
  "dependencies": {
    "sequelize": "^6.33.0",
    "mysql2": "^3.6.1",
    "dotenv": "^16.3.1",
