{
  "name": "network-cmdb-backend",
  "version": "1.0.0",
  "description": "Network CMDB Backend API with MySQL integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:health": "ts-node src/utils/db-health-check.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "ajv": "^6.12.6",
    "ajv-formats": "^3.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "joi": "^17.10.2",
    "jschardet": "^3.1.4",
    "multer": "^2.0.2",
    "mysql2": "^3.14.4",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^24.19.0",
    "redis": "^4.6.8",
    "sequelize": "^6.37.7",
    "stream-json": "^1.9.1",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.1",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "sequelize-cli": "^6.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "network",
    "cmdb",
    "mysql",
    "sequelize",
    "typescript",
    "api",
    "backend"
  ],
  "license": "MIT"
}
