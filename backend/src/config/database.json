{
  "development": {
    "username": "root",
    "password": null,
    "database": "networkdb_dev",
    "host": "127.0.0.1",
    "port": 3306,
    "dialect": "mysql",
    "dialectOptions": {
      "charset": "utf8mb4",
      "collate": "utf8mb4_unicode_ci"
    },
    "pool": {
      "max": 10,
      "min": 0,
      "acquire": 30000,
      "idle": 10000
    },
    "logging": console.log
  },
  "test": {
    "username": "root",
    "password": null,
    "database": "networkdb_test",
    "host": "127.0.0.1",
    "port": 3306,
    "dialect": "mysql",
    "dialectOptions": {
      "charset": "utf8mb4",
      "collate": "utf8mb4_unicode_ci"
    },
    "pool": {
      "max": 5,
      "min": 0,
      "acquire": 30000,
      "idle": 10000
    },
    "logging": false
  },
  "production": {
    "username": "${DB_USERNAME}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_NAME}",
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "dialect": "mysql",
    "dialectOptions": {
      "charset": "utf8mb4",
      "collate": "utf8mb4_unicode_ci",
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "pool": {
      "max": 20,
      "min": 5,
      "acquire": 30000,
      "idle": 10000
    },
    "logging": false
  }
}